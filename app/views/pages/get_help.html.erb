<div class="title">
  <h1 class="textCenter">Reach out for help:</h1>
</div>
<div id="getHelpOptions">
  <% iter = 1 %>
    <% @contacts.each do |contact| %>
      <div class="card cardHeader" id="choice<%= iter %>"><%= contact.name == "" ? "Empty contact" : contact.name %></div>
      <% iter += 1 %>
    <% end %>
</div>
<%= link_to "Get Help", send_path, method: :post, class: "charliesBtn" %>